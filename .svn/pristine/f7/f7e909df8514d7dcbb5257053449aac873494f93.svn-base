<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape >
            <solid android:color="@color/mgp_back_btn_press"/>
            <stroke android:width="1px" android:color="@color/mgp_sdk_color"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape >
            <solid android:color="@color/mgp_back_btn_press"/>
            <stroke android:width="1px" android:color="@color/mgp_sdk_color"/>
        </shape>
    </item>
    <item >
        <shape >
            <solid android:color="@color/mgp_sdk_color"/>
            <stroke android:width="1px" android:color="@color/mgp_sdk_color"/>
        </shape>
    </item>
</selector>
