<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Non focused states -->
   
    <item android:state_focused="false" android:state_selected="false" android:state_pressed="false" android:drawable="@drawable/msp_check_box_normal_n" />
    <item android:state_focused="false" android:state_selected="true" android:state_pressed="false" android:drawable="@drawable/msp_check_box_normal_n" />
    
    <!-- Focused states -->
    
    <item android:state_focused="true" android:state_selected="false" android:state_pressed="false" android:drawable="@drawable/msp_check_box_normal_h" />
    <item android:state_focused="true" android:state_selected="true" android:state_pressed="false" android:drawable="@drawable/msp_check_box_normal_h" />
    
    <!-- Pressed -->
       <item android:drawable="@drawable/msp_check_box_normal_h" android:state_pressed="true"/>

</selector>